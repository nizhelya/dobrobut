UPDATE `ADDRESS` SET  osmd_id = 6  WHERE `house_id` =41
UPDATE `APPARTMENT` SET osmd_id = 6  WHERE `house_id` =41
UPDATE `KVARTPLATA` SET osmd_id = 6  WHERE `house_id` =41 and osmd = 1
UPDATE `BKVARTPLATA` SET osmd_id = 6  WHERE `house_id` =41 and osmd = 1
UPDATE `OPLATA` SET  osmd_id = 6  WHERE `house_id` =41 and osmd = 1  and data >= '20170101'


