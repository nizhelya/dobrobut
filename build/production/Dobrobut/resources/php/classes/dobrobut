UPDATE `HOUSE` SET  `osmd` =1,osmd_id = 24  WHERE `house_id` in(4,5,6,9,16,25,26,27,41,48,72)
UPDATE `APPARTMENT`SET  `osmd` =1,osmd_id = 24  WHERE `house_id` in(4,5,6,9,16,25,26,27,41,48,72)
UPDATE `KVARTPLATA` SET  `osmd` =1,osmd_id = 24  WHERE `house_id` in(4,5,6,9,16,25,26,27,41,48,72) and data = '20170501'
UPDATE `BKVARTPLATA` SET  `osmd` =1,osmd_id = 24  WHERE `house_id` in(4,5,6,9,16,25,26,27,41,48,72) and data = '20170501'
UPDATE `OPLATA` SET  `osmd` =1,osmd_id = 24  WHERE `house_id` in(4,5,6,9,16,25,26,27,41,48,72) and data =>'20170401'

UPDATE YIS.OPLATA as t1 ,YIS.HOUSE as t2  SET t1.data_in =  t1.data_in, t1.osmd_id = t2.osmd_id  WHERE t1.`house_id` = t2.house_id and t1.data >='20170101' and t1.osmd = 1 and  t1.`kvartplata` != 0

UPDATE OPLATA as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id and  t1.`data`BETWEEN '20170101' and '20170430' AND  t1.`osmd` = 1 AND   t1.`dolg` = 0  and t1.`kvartplata` != 0 V
UPDATE APPARTMENT as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id V
UPDATE ADDRESS as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id V

UPDATE KVARTPLATA as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id and  t1.`data`BETWEEN '20170101' and '20170501' 
UPDATE BKVARTPLATA as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id and  t1.`data`BETWEEN '20170101' and '20170401' V
UPDATE PAYMENT as t1 , HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id and  t1.`data`BETWEEN '20170101' and '20170401' V

UPDATE YISGRAND.MTB_PAYMENT  as t1 , YIS.HOUSE as t2  SET  t1.`osmd_id` =t2.osmd_id  WHERE t1.`house_id` = t2.house_id and  t1.`data`BETWEEN '20170101' and '20170401' V
